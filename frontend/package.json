{
  "name": "ebay-sales-tool-frontend",
  "version": "1.0.0",
  "description": "Frontend for eBay sales tool to assist in selling and tracking items on eBay.",
  "main": "src/App.js",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=4096 && react-scripts start",
    "build": "set NODE_OPTIONS=--max-old-space-size=4096 && react-scripts build",
    
    "eject": "react-scripts eject",
    "prepare-lock": "npm install --package-lock-only"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "ebay-sales-tool-frontend": "file:",
    "ebaysalestool": "file:..",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "react-scripts": "^5.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
