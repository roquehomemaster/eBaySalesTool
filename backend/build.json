{
  "runApiTests": true,
  "database": {
    "host": "localhost",
    "host_local": "localhost",
    "user": "postgres",
    "password": "password",
  "database": "listflowhq",
    "port": 5432
  },
  "constants": {
    "maxRetries": 10,
    "retryDelay": 20000,
    "healthCheckRetries": 20,
    "healthCheckDelay": 10000
  },
  "paths": {
  "backend": "f:/Dev/ListFlowHQ/backend",
  "frontend": "f:/Dev/ListFlowHQ/frontend",
  "buildScripts": "f:/Dev/ListFlowHQ/build/scripts",
  "dockerComposeFile": "f:/Dev/ListFlowHQ/docker-compose.yml"
  },
  "testdata": true,
  "network": {
    "subnet": "192.168.0.0/24",
    "postgresDbIp": "postgres_db",
    "backendIp": "192.168.0.221",
    "frontendIp": "192.168.0.222"
  },
  "build": {
    "backend": {
      "build": true
    },
    "frontend": {
      "build": true
    },
    "docker": {
      "use_compose": true
    },
    "build.json": {
      "version": "1.0.0",
      "description": "Build configuration for eBay Sales Tool",
      "backend": {
        "entry": "backend/src/app.js",
        "output": "dist/backend"
      },
      "frontend": {
        "entry": "frontend/src/index.js",
        "output": "dist/frontend"
      }
    }
  }
}